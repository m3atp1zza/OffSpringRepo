<extension point="xbmc.addon.metadata">
  <summary lang="en_GB">TheMovieDb Helper - Now With Trakt Support!</summary>
  <summary lang="es_ES">TheMovieDb Helper - ¡Ahora con soporte para Trakt!</summary>
